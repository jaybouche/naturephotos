<br />

<div class="booyah-box col-10 offset-1">
  <h1 class="text-center">Nature Photos</h1>
  <hr />

<% @photos.each do |photo| %>
    <h2 class="text-center">
    <%= link_to photo.caption, photos_path(photo) %>
    <%= image_tag photo.image.to_s, class: 'img-fluid' %>
  <br /> 
<% end %>

</div>